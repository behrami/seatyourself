<%= form_for user, url: new_user_url do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :password %>
  <%= f.text_field :password_digest %>

  <%= f.label :password_confirmation %>
  <%= f.text_field :password_digest %>

  <%= f.submit :submit %>

<% end %>
